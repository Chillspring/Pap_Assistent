@model PaP_Assisstent.Models.InGameModel

@{
    ViewBag.Title = "LoggedIn";
}

<h2>Logged in as </h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Players.First().Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Players.First().Password)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Players)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
        </tr>
    }

</table>
<input type="button" value="Logout" class="btn btn-default" id="logout" onclick="location.href='@Url.Action("Logout_Click", "Login")'" />